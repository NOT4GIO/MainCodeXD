function Find()
 if game.PlaceId == 11866898599 then -- id here 
    print("Success!")
 else
     if game.PlaceId == 5602055394 then-- id here 
         -- loadstring here
     else
         if game.PlaceId == 2788229376 then-- id here 
             -- loadstring here
         else
             if game.PlaceId == 9824221333 then -- id here 
                 -- loadstring here
             else
                 if game.PlaceId == 9825515356 then-- id here 
                     -- loadstring here
                 else

                     game:GetService("Players").LocalPlayer:Kick("Game Isnt supported.")
                 end
             end
         end
     end
 end
end


if table.find(AllowedToPlay, tostring(game.PlaceId)) then
 Find()
else
 game:GetService("Players").LocalPlayer:Kick("Game isnt supported")
end  


